{
  "name": "blackbeetle-backend_nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node -r tsconfig-paths/register --env-file=.env.development node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --env-file=.env.test node_modules/.bin/jest",
    "test:watch": "node --env-file=.env.test node_modules/.bin/jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --env-file=.env.test --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node --env-file=.env.test node_modules/.bin/jest --config ./test/jest-e2e.json",
    "typeorm": "node -r tsconfig-paths/register --env-file=.env.development node_modules/typeorm/cli-ts-node-commonjs.js -d ./src/database/typeorm.config.ts",
    "typeorm:show": "node -r tsconfig-paths/register --env-file=.env.development node_modules/typeorm/cli-ts-node-commonjs.js migration:show -d ./src/database/typeorm.config.ts",
    "typeorm:migrate": "node -r tsconfig-paths/register --env-file=.env.development node_modules/typeorm/cli-ts-node-commonjs.js migration:run -d ./src/database/typeorm.config.ts",
    "typeorm:generate": " node -r tsconfig-paths/register --env-file=.env.development node_modules/typeorm/cli-ts-node-commonjs.js migration:generate ./src/database/migrations/initial-schema -d ./src/database/typeorm.config.ts -o",
    "typeorm:drop": " node -r tsconfig-paths/register --env-file=.env.development node_modules/typeorm/cli-ts-node-commonjs.js schema:drop -d ./src/database/typeorm.config.ts",
    "db:rebuild": "npm run typeorm:drop && npm run typeorm:generate && npm run typeorm:migrate",
    "seed": "node --env-file=.env.development -r tsconfig-paths/register -r ts-node/register ./src/database/seed/bootstrap.seed.ts"
  },
  "dependencies": {
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "bullmq": "^5.12.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
